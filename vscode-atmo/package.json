{
  "name": "vscode-atmo",
  "displayName": "vscode-atmo",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [ "onFileSystem:atmo", "workspaceContains:*.ts", "workspaceContains:*.go", "workspaceContains:*.at", "workspaceContains:*.go.at", "workspaceContains:*.ts.at" ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-atmo.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.46.3",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
